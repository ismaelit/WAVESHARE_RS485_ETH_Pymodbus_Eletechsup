{
  "gateway_config": {
    "device_name": "Waveshare RS485-TO-ETH-B",
    "description": "Configuração para Módulo 2 - 4 portas apenas saídas digitais",
    "version": "1.0",
    "created": "2024-12-19"
  },
  
  "mqtt_settings": {
    "server_ip": "172.9.2.11",
    "server_port": 1883,
    "username": "choppers",
    "password": "ismaisma",
    "client_id": "modulo2_gateway",
    "publish_topic": "modbus/modulo2/status",
    "subscribe_topic": "modbus/modulo2/control",
    "keep_alive": 60,
    "server_upload_time": 500
  },
  
  "modbus_rtu_settings": {
    "baud_rate": 9600,
    "data_bits": 8,
    "stop_bits": 1,
    "parity": "none",
    "slave_address": 2,
    "function_code": 3,
    "polling_interval": 500
  },
  
  "json_structure": {
    "device_id": "modulo2_25iob16",
    "timestamp": "auto_ntp",
    "data_nodes": [
      {
        "id": "1",
        "json_key": "saida_1",
        "data_source": "modbus_rtu",
        "modbus_register": 0,
        "data_length": 2,
        "data_format": "unsigned_int",
        "decimal_places": 0,
        "unit": "",
        "add_quotes": false,
        "description": "Status da saída 1"
      },
      {
        "id": "2",
        "json_key": "saida_2",
        "data_source": "modbus_rtu",
        "modbus_register": 1,
        "data_length": 2,
        "data_format": "unsigned_int",
        "decimal_places": 0,
        "unit": "",
        "add_quotes": false,
        "description": "Status da saída 2"
      },
      {
        "id": "3",
        "json_key": "saida_3",
        "data_source": "modbus_rtu",
        "modbus_register": 2,
        "data_length": 2,
        "data_format": "unsigned_int",
        "decimal_places": 0,
        "unit": "",
        "add_quotes": false,
        "description": "Status da saída 3"
      },
      {
        "id": "4",
        "json_key": "saida_4",
        "data_source": "modbus_rtu",
        "modbus_register": 3,
        "data_length": 2,
        "data_format": "unsigned_int",
        "decimal_places": 0,
        "unit": "",
        "add_quotes": false,
        "description": "Status da saída 4"
      }
    ]
  },
  
  "output_format": {
    "example": {
      "device_id": "modulo2_25iob16",
      "timestamp": "2024-12-19 15:30:45",
      "saida_1": 256,
      "saida_2": 0,
      "saida_3": 256,
      "saida_4": 0
    }
  }
}
